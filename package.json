{
  "name": "expo-boilerplate",
  "version": "1.0.0",
  "main": "expo-router/entry",
  "scripts": {
    "dev": "expo start --clear",
    "dev:ios": "expo start --ios",
    "dev:android": "expo start --android",
    "dev:web": "expo start --web",
    "dev:doctor": "expo doctor",
    "dev:build:mobile": "eas build --platform all --profile development",
    "dev:build:web": "expo export --platform web",
    "dev:serve:web": "npx serve dist",
    "dev:deploy:web": "expo export --platform web && eas update",
    "dev:secret:push": "eas secret:push --scope project --env-file .env.dev",
    "dev:secret:list": "eas secret:list",
    "dev:config:public": "expo config --type public",
    "lint": "eslint .",
    "lint:staged": "lint-staged",
    "format": "prettier --write .",
    "test": "jest",
    "test:watch": "jest --watch",
    "prepare": "husky install"
  },
  "dependencies": {
    "@expo/vector-icons": "^14.0.4",
    "@reduxjs/toolkit": "^2.5.0",
    "@supabase/supabase-js": "^2.48.0",
    "expo": "~53.0.0",
    "expo-auth-session": "~6.2.1",
    "expo-crypto": "~14.1.5",
    "expo-font": "~13.3.2",
    "expo-linking": "~7.1.7",
    "expo-router": "~5.1.5",
    "expo-secure-store": "~14.2.4",
    "expo-splash-screen": "~0.30.10",
    "expo-status-bar": "~2.2.3",
    "expo-web-browser": "~14.2.0",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-native": "0.79.5",
    "react-native-gesture-handler": "~2.24.0",
    "react-native-reanimated": "~3.17.4",
    "react-native-safe-area-context": "5.4.0",
    "react-native-screens": "~4.11.1",
    "react-native-svg": "15.11.2",
    "react-redux": "^9.1.2"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react-native": "^12.8.1",
    "@types/jest": "^29.5.14",
    "@types/react": "~19.0.0",
    "@types/react-test-renderer": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react": "^7.37.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-native": "^4.1.0",
    "globals": "^13.24.0",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "jest-expo": "~53.0.10",
    "lint-staged": "^15.2.10",
    "prettier": "^3.3.3",
    "react-test-renderer": "19.0.0",
    "typescript": "~5.8.3"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "private": true
}
